{
  "name": "express-vue",
  "version": "5.0.1",
  "description": "Vue rendering engine for Express.js",
  "homepage": "https://github.com/express-vue/express-vue",
  "author": {
    "name": "Daniel Cherubini",
    "email": "daniel@cherubini.casa",
    "url": "https://cherubini.casa"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "keywords": [
    "vue",
    "express",
    "node"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/example/**/*.js"
    ],
    "source": [
      "lib/**/*.js"
    ],
    "concurrency": 5,
    "failFast": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "exclude": [
      "**/*.vue"
    ],
    "include": [
      "lib/**/*.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^9.4.5",
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "codecov": "^3.0.0",
    "del": "^3.0.0",
    "eslint": "^4.17.0",
    "eslint-config-xo-space": "^0.17.0",
    "eslint-plugin-babel": "^4.1.2",
    "express": "^4.16.1",
    "generate-release": "^0.14.0",
    "ncp": "^2.0.0",
    "nsp": "^3.1.0",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "mocha": true
    }
  },
  "repository": "express-vue/express-vue",
  "scripts": {
    "start": "node test/example/index.js",
    "debug": "node --inspect test/example/index.js",
    "release": "generate-release",
    "lint": "tslint --project tsconfig.json",
    "test": "npm run lint && nsp check && nyc ava",
    "preversion": "npm test",
    "version": "git add -A",
    "postversion": "git push && git push --tags",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "debug": "^3.1.0",
    "vue": "^2.5.13",
    "vue-pronto": "^1.0.3",
    "vue-template-compiler": "^2.5.13"
  }
}
