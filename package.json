{
  "name": "express-vue",
  "version": "5.10.0",
  "description": "Vue rendering engine for Express.js",
  "homepage": "https://github.com/express-vue/express-vue",
  "author": {
    "name": "Daniel Cherubini",
    "email": "daniel@cherubini.casa",
    "url": "https://cherubini.casa"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "bin": {
    "express-vue": "bin/index.js"
  },
  "keywords": [
    "vue",
    "express",
    "node"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/example/**/*.js"
    ],
    "sources": [
      "lib/**/*.js"
    ],
    "failFast": false,
    "powerAssert": false,
    "babel": {
      "testOptions": {
        "babelrc": false,
        "configFile": false
      }
    }
  },
  "nyc": {
    "exclude": [
      "**/*.vue"
    ],
    "include": [
      "lib/**/*.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@types/node": "^10.14.12",
    "ava": "^1.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.2",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "codecov": "^3.5.0",
    "del": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-xo-space": "^0.21.0",
    "eslint-plugin-babel": "^5.3.0",
    "express": "^4.17.1",
    "generate-release": "^1.1.1",
    "ncp": "^2.0.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "mocha": true
    }
  },
  "repository": "express-vue/express-vue",
  "scripts": {
    "start": "node test/example/index.js",
    "debug": "VUE_DEV=true node --inspect test/example/index.js",
    "release": "npm test && generate-release",
    "lint": "tslint --project tsconfig.json",
    "test": "npm run lint && nyc ava",
    "preversion": "npm test",
    "version": "git add -A",
    "postversion": "git push && git push --tags",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@types/yargs": "^13.0.0",
    "chalk": "^2.4.2",
    "figlet": "^1.2.3",
    "vue-pronto": "^1.11.0"
  }
}
